{
  "name": "myportfolio",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "imagemin-pngquant": "^5.0.1",
    "nanoid": "^3.1.10",
    "node-sass": "^4.14.1",
    "pngquant-bin": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.2.0",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.7.16",
    "react-tilt": "^0.1.4",
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.3.0",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
